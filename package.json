{
  "name": "front-end-app",
  "version": "1.8.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "test:coverage": "pnpm run coverage",
    "lint": "eslint \"src/**/*.{ts,html}\"",
    "format:check": "prettier --check .",
    "lint:fix": "eslint 'src/**/*.{ts,html}' --fix",
    "prettier:write": "prettier --write \"src/**/*.{ts,html,json,css,scss}\"",
    "format": "prettier --write .",
    "prepare": "husky install",
    "increment-version": "pnpm dlx ts-node --project tsconfig.scripts.json scripts/bump-version.ts"
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "pnpm lint",
      "git add"
    ],
    "src/**/*.{ts,html,scss,json,css,md,yml,yaml}": [
      "pnpm prettier:write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "pnpm": {
    "overrides": {
      "@modelcontextprotocol/sdk": "1.25.2"
    }
  },
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.7",
    "@angular/core": "^21.0.7",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.4",
    "@angular/router": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.1.0",
    "@angular-eslint/eslint-plugin": "21.1.0",
    "@angular-eslint/eslint-plugin-template": "21.1.0",
    "@angular-eslint/schematics": "21.1.0",
    "@angular-eslint/template-parser": "21.1.0",
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.4",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "4.0.16",
    "angular-eslint": "^21.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "~5.9.2",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
